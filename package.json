{
  "name": "Guzl",
  "version": "0.0.1",
  "description": "App for guzzling content",
  "main": "main.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"electron .\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "dist": "build -mwl --x64 --ia32"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kr4ng/guzl.git"
  },
  "keywords": [
    "Guzl",
    "Reddit",
    "Porn",
    "Fapping",
    "Jerking"
  ],
  "author": "Steven Simoni <steven.m.simoni@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kr4ng/guzl/issues"
  },
  "homepage": "https://github.com/kr4ng/guzl/blob/master/README.md",
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/platform-server": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@angular/upgrade": "^4.2.4",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.1",
    "systemjs": "0.19.39",
    "typescript": "2.3.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/node": "6.0.45",
    "concurrently": "3.0.0",
    "electron": "1.6.2",
    "electron-builder": "^19.8.0",
    "typescript": "2.3.4"
  },
  "build": {
    "appId": "hr.creaticon.guzl",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  }
}
